   ],
   "source": [
    "data = open('inputs/day_1', 'r').read().split('\\n')[0:-1]\n",
    "freqs = [int(d[1::]) if d[0]=='+' else -1 * int(d[1::]) for d in data]\n",
    "sum(freqs)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Part 2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "freq = 0\n",
    "seen = set()\n",
    "\n",
    "for i in freqs:\n",
    "    freq += i\n",
    "    if freq in seen: \n",
    "        break\n",
    "    seen.add(freq)"
   ]
  }
